<+><?xml version=\"1.0\" encoding=\"UTF-8\"?>\r\n<project xmlns=\"http://maven.apache.org/POM/4.0.0\"\r\n         xmlns:xsi=\"http://www.w3.org/2001/XMLSchema-instance\"\r\n         xsi:schemaLocation=\"http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd\">\r\n    <modelVersion>4.0.0</modelVersion>\r\n\r\n    <!--  项目坐标  -->\r\n    <groupId>org.hret</groupId>\r\n    <!--  项目名称  -->\r\n    <artifactId>oa-system</artifactId>\r\n    <!--  项目版本  -->\r\n    <version>1.0</version>\r\n\r\n    <!--  项目打包方式  pom：项目聚合  -->\r\n    <packaging>pom</packaging>\r\n\r\n    <!--  项目模块  -->\r\n    <modules>\r\n        <module>oa-system-common</module>\r\n        <module>oa-system-web</module>\r\n        <module>oa-system-security</module>\r\n        <module>oa-system-mapper</module>\r\n        <module>oa-system-service</module>\r\n        <module>oa-system-bean</module>\r\n    </modules>\r\n\r\n    <!--  项目描述 | 版本控制  -->\r\n    <properties>\r\n        <maven.compiler.source>17</maven.compiler.source>\r\n        <maven.compiler.target>17</maven.compiler.target>\r\n        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>\r\n\r\n        <!--  统一版本  -->\r\n        <!-- springboot版本 -->\r\n        <spring.boot.version>3.1.0</spring.boot.version>\r\n        <!-- spring_doc版本 api文档 -->\r\n        <spring-doc.version>2.2.0</spring-doc.version>\r\n        <!-- lombok版本 实体类生成get set方法等等 -->\r\n        <lombok.version>1.18.20</lombok.version>\r\n        <!-- fastjson版本 json解析 -->\r\n        <fastjson.version>1.2.83</fastjson.version>\r\n        <!-- huToolAll版本 工具类 -->\r\n        <hutool.all.version>5.6.2</hutool.all.version>\r\n        <!-- mysql版本 -->\r\n        <mysql.version>8.0.32</mysql.version>\r\n        <!-- mybatis-plus版本 -->\r\n        <mybatis-plus.version>3.5.3.1</mybatis-plus.version>\r\n        <!-- druid版本 数据库连接池 -->\r\n        <druid.version>1.1.21</druid.version>\r\n        <!-- pageHelper版本 分页插件 -->\r\n        <pagehelper.version>1.4.6</pagehelper.version>\r\n        <!-- redis版本 -->\r\n        <redis.version>3.1.0</redis.version>\r\n        <!-- redis连接池版本 -->\r\n        <commons-pool2.version>2.11.1</commons-pool2.version>\r\n        <!-- spring-session-redis版本 -->\r\n        <spring-session-redis.version>3.1.0</spring-session-redis.version>\r\n        <!-- knife4j版本 api文档增强 -->\r\n        <knife4j.version>4.1.0</knife4j.version>\r\n    </properties>\r\n\r\n    <!--  全局依赖管理  -->\r\n    <dependencyManagement>\r\n        <dependencies>\r\n            <dependency>\r\n                <groupId>org.hret</groupId>\r\n                <artifactId>oa-system-web</artifactId>\r\n                <version>${project.version}</version>\r\n            </dependency>\r\n            <dependency>\r\n                <groupId>org.hret</groupId>\r\n                <artifactId>oa-system-bean</artifactId>\r\n                <version>${project.version}</version>\r\n            </dependency>\r\n            <dependency>\r\n                <groupId>org.hret</groupId>\r\n                <artifactId>oa-system-common</artifactId>\r\n                <version>${project.version}</version>\r\n            </dependency>\r\n            <dependency>\r\n                <groupId>org.hret</groupId>\r\n                <artifactId>oa-system-mapper</artifactId>\r\n                <version>${project.version}</version>\r\n            </dependency>\r\n            <dependency>\r\n                <groupId>org.hret</groupId>\r\n                <artifactId>oa-system-security</artifactId>\r\n                <version>${project.version}</version>\r\n            </dependency>\r\n            <dependency>\r\n                <groupId>org.hret</groupId>\r\n                <artifactId>oa-system-service</artifactId>\r\n                <version>${project.version}</version>\r\n            </dependency>\r\n\r\n            <!-- springboot依赖管理 -->\r\n            <dependency>\r\n                <groupId>org.springframework.boot</groupId>\r\n                <artifactId>spring-boot-dependencies</artifactId>\r\n                <version>${spring.boot.version}</version>\r\n                <type>pom</type>\r\n                <scope>import</scope>\r\n            </dependency>\r\n            <!-- lombok依赖管理 -->\r\n            <dependency>\r\n                <groupId>org.projectlombok</groupId>\r\n                <artifactId>lombok</artifactId>\r\n                <version>${lombok.version}</version>\r\n            </dependency>\r\n            <!-- fastjson依赖管理 -->\r\n            <dependency>\r\n                <groupId>com.alibaba</groupId>\r\n                <artifactId>fastjson</artifactId>\r\n                <version>${fastjson.version}</version>\r\n            </dependency>\r\n            <!-- huToolAll依赖管理 -->\r\n            <dependency>\r\n                <groupId>cn.hutool</groupId>\r\n                <artifactId>hutool-all</artifactId>\r\n                <version>${hutool.all.version}</version>\r\n            </dependency>\r\n            <!-- mysql依赖管理 -->\r\n            <dependency>\r\n                <groupId>mysql</groupId>\r\n                <artifactId>mysql-connector-java</artifactId>\r\n                <version>${mysql.version}</version>\r\n            </dependency>\r\n            <!-- mybatis-plus依赖管理 -->\r\n            <dependency>\r\n                <groupId>com.baomidou</groupId>\r\n                <artifactId>mybatis-plus-boot-starter</artifactId>\r\n                <version>${mybatis-plus.version}</version>\r\n            </dependency>\r\n            <!-- druid依赖管理 -->\r\n            <dependency>\r\n                <groupId>com.alibaba</groupId>\r\n                <artifactId>druid-spring-boot-starter</artifactId>\r\n                <version>${druid.version}</version>\r\n            </dependency>\r\n            <!-- pageHelper依赖管理 -->\r\n            <dependency>\r\n                <groupId>com.github.pagehelper</groupId>\r\n                <artifactId>pagehelper-spring-boot-starter</artifactId>\r\n                <version>${pagehelper.version}</version>\r\n            </dependency>\r\n            <!-- redis依赖管理 -->\r\n            <dependency>\r\n                <groupId>org.springframework.boot</groupId>\r\n                <artifactId>spring-boot-starter-data-redis</artifactId>\r\n                <version>${redis.version}</version>\r\n            </dependency>\r\n            <!-- redis连接池依赖管理 -->\r\n            <dependency>\r\n                <groupId>org.apache.commons</groupId>\r\n                <artifactId>commons-pool2</artifactId>\r\n                <version>${commons-pool2.version}</version>\r\n            </dependency>\r\n            <!-- spring-session-redis依赖管理 -->\r\n            <dependency>\r\n                <groupId>org.springframework.session</groupId>\r\n                <artifactId>spring-session-data-redis</artifactId>\r\n                <version>${spring-session-redis.version}</version>\r\n            </dependency>\r\n            <!-- 接口文档 -->\r\n            <dependency>\r\n                <groupId>com.github.xiaoymin</groupId>\r\n                <artifactId>knife4j-openapi3-jakarta-spring-boot-starter</artifactId>\r\n                <version>${knife4j.version}</version>\r\n            </dependency>\r\n        </dependencies>\r\n    </dependencyManagement>\r\n\r\n    <!--统一环境-->\r\n    <profiles>\r\n        <profile>\r\n            <id>prod</id>\r\n            <properties>\r\n                <profiles.active>prod</profiles.active>\r\n            </properties>\r\n            <activation>\r\n                <activeByDefault>true</activeByDefault>\r\n            </activation>\r\n        </profile>\r\n        <profile>\r\n            <id>dev</id>\r\n            <properties>\r\n                <profiles.active>dev</profiles.active>\r\n            </properties>\r\n        </profile>\r\n    </profiles>\r\n\r\n\r\n    <!--maven插件-->\r\n    <build>\r\n        <plugins>\r\n            <plugin>\r\n                <groupId>org.springframework.boot</groupId>\r\n                <artifactId>spring-boot-maven-plugin</artifactId>\r\n                <configuration>\r\n                    <mainClass>none</mainClass>     <!-- 取消查找本项目下的Main方法：为了解决Unable to find main class的问题 -->\r\n                    <classifier>execute</classifier>    <!-- 为了解决依赖模块找不到此模块中的类或属性 -->\r\n                </configuration>\r\n                <executions>\r\n                    <execution>\r\n                        <goals>\r\n                            <goal>repackage</goal>\r\n                        </goals>\r\n                    </execution>\r\n                </executions>\r\n            </plugin>\r\n        </plugins>\r\n    </build>\r\n\r\n</project>
